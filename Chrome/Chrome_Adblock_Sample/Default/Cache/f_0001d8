  color: #393436;
  text-decoration: underline;
  cursor: pointer;
}
a:hover,
a:focus {
  color: #111010;
  text-decoration: none;
}
a:focus {
  outline: thin dotted;
  outline: 5px auto -webkit-focus-ring-color;
  outline-offset: -2px;
}
