  font-family: 'Proxima Nova Rg';
}
.font-medium {
  font-family: 'Proxima Nova Lt';
}
.font-bold {
  font-family: 'Proxima Nova Bold';
}
.font-black {
  font-family: 'Proxima Nova Bl';
}
