<script type="text/javascript">			
	function toggleDropdown(event, id) {
		event.preventDefault();
		if ($(id+'HeaderItem').hasClass('activeCategory'))
			closeDropdown(id);
		else {
			if ($('.activeCategory').size()>0) {	
				$('.linksDropDown').hide();
				$('.activeCategory').removeClass('activeCategory');
				$(id).show();
			} else
			$(id).slideDown();	
			$(id+'HeaderItem').toggleClass('activeCategory');
		}
		return false;
	}
</script>
			
