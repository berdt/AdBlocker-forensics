<script type="text/javascript">
	function resetFeatureRotator() {
		if (Modernizr.mq('only screen and (max-width : 959px)')) {
			setTimeout(function(){$('#featureRotator').carousel('pause');}, 5000);		        
    	} else {
    		if (!Modernizr.touch) {
    			$('#featureRotator').carousel('cycle');
    		} 
    	}
	}
	
	$(document).ready(function() {		 
    	if (false && Modernizr.touch)  {
			var rotatorDiv = document.getElementById('innerCarouselDiv');		
			Hammer(rotatorDiv).on("swipeleft", function() {						
				$('#featureRotator').carousel('next');			
			});		
	   		Hammer(rotatorDiv).on("swiperight", function() {	    	
	    		$('#featureRotator').carousel('prev');
	    	});	
    	}  
    	resetFeatureRotator();
    	
        $('.photoButton').click(function(){
        	$('#featureRotator').carousel("pause");
	        var restart = setInterval(function(){	            
	            $('#featureRotator').carousel("cycle");
	            clearInterval(restart);	            
	        }, 30000);        
        });
        
        $(window).resize(function() {
			resetFeatureRotator();
		});	
        
        //test here...
        $('#featureRotator').bind('slide.bs.carousel', function (e) {            
            $('#innerNavContainer').hide();
        });
        
        $('#featureRotator').bind('slid.bs.carousel', function (e) {            
            $('#innerNavContainer').show();
        });
        
	});//end document.ready function
	
</script></div>
	
		</div>
	</div>
