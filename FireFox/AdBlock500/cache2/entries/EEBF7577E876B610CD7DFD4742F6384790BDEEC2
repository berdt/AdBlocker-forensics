		if(jQuery(".slctB").css("display")=="block"){
			jQuery(".slctB:not(:animated)").slideUp(300);
		}else{
			jQuery(".slctB").slideDown(300);
		}
	});

	//toggle
	jQuery(".searchToggle").click(function(){
		if (jQuery(this).hasClass('searchToggle-down')) {
		jQuery(this).removeClass('searchToggle-down').addClass('searchToggle-up');
	}
	else {
		jQuery(this).removeClass('searchToggle-up').addClass('searchToggle-down');
	}
	});

	//hover
	jQuery("#domin01 li").hover(function(){
		jQuery(this).addClass("hoverC");
	},function(){
		jQuery(this).removeClass("hoverC");
	})

	jQuery("#domin02 li").hover(function(){
		jQuery(this).addClass("hoverC");
	},function(){
		jQuery(this).removeClass("hoverC");
	})

