		var locationurl = jQuery('.btn_002 > a').attr('href');
		window.open(locationurl);
	});


	function rotateScroll(iD) {
		hrefData = '#tab'+tabFlg;
		jQuery('#rttnBnnr > ul > li > a').attr('style','background-image: url(//cache.img.gmo.jp/onamae/images/RotationBanner_tab.gif);');
		jQuery('#tb'+tabFlg+' > a').attr('style','background-image: url(//cache.img.gmo.jp/onamae/images/RotationBanner_tab_ov.gif);color:#fff;');
		switch(hrefData){
		case '#tab1':
		jQuery('#rotation_inner').animate({left: '0px'}, 600, "easeOutQuart");
		jQuery('.btn_002 > a').attr('href','http://onamae-premium.com/onamae/premiumDomain/select/?banner_id=831_top_premium-banner_japanese&#japanese');
		break;
		
		case '#tab2':
		jQuery('#rotation_inner').animate({left: '-918px'}, 600, "easeOutQuart");
		jQuery('.btn_002 > a').attr('href','http://onamae-premium.com/onamae/premiumDomain/select/?banner_id=831_top_premium-banner_pop&#pop');
//		jQuery('#rotation_inner').attr('style','left:-842px');
		break;
		
		case '#tab3':
		jQuery('#rotation_inner').animate({left: '-1836px'}, 600, "easeOutQuart");
		jQuery('.btn_002 > a').attr('href','http://onamae-premium.com/onamae/premiumDomain/select/?banner_id=831_top_premium-banner_business&#business');
		break;
		
		case '#tab4':
		jQuery('#rotation_inner').animate({left: '-2754px'}, 600, "easeOutQuart");
		jQuery('.btn_002 > a').attr('href','http://onamae-premium.com/onamae/premiumDomain/select/?banner_id=831_top_premium-banner_food&#food');
		break;

		case '#tab5':
		jQuery('#rotation_inner').animate({left: '-3672px'}, 600, "easeOutQuart");
		jQuery('.btn_002 > a').attr('href','http://onamae-premium.com/onamae/premiumDomain/select/?banner_id=831_top_premium-banner_super&#super');
		break;

		}

		tabFlg++;
		if ( tabFlg > 5 ) tabFlg = 1;
		//lmtsflg = setTimeout(rotateScroll, rotateTime );			/* auctionƒoƒi[ŽžŽ~‚ß‚é */


		return false;
	}  

	jQuery('#auction_banner').click(function(){
		location.href = "http://auction.onamae.com/onamae/search.cgi?acc=category&ct_no=01&btn_id=1_auction";
		return false;
	});

	var auction_animete_flg = 3;
	jQuery('#ctgrty0'+auction_animete_flg).animate({left: '0px'}, 1000, "easeOutQuart");
	var auction_animete_lmtsflg = setTimeout(auction_animete, rotateTime );
	function auction_animete () {
		clearTimeout(auction_animete_lmtsflg);
		jQuery('#ctgrty0'+auction_animete_flg).animate({left: '400px'}, 1000, "easeOutQuart");
		jQuery('#ctgrty0'+auction_animete_flg).attr('style', 'left: -500px');
		auction_animete_flg++;
		if ( auction_animete_flg > 3 ) auction_animete_flg = 1;
		jQuery('#ctgrty0'+auction_animete_flg).attr('style', 'left: -500px').animate({left: '0px'}, 1000, "easeOutQuart");
		auction_animete_lmtsflg = setTimeout(auction_animete, rotateTime );
	}



});

/*=====================================================
meta: {
  title: "jquery-opacity-rollover.js",
  version: "2.1",
  copy: "copyright 2009 h2ham (h2ham.mail@gmail.com)",
  license: "MIT License(http://www.opensource.org/licenses/mit-license.php)",
  author: "THE HAM MEDIA - http://h2ham.seesaa.net/",
  date: "2009-07-21"
  modify: "2009-07-23"
}
=====================================================*/
(function($) {
	
	$.fn.opOver = function(op,oa,durationp,durationa){
		
		var c = {
			op:op? op:1.0,
			oa:oa? oa:0.6,
			durationp:durationp? durationp:'fast',
			durationa:durationa? durationa:'fast'
		};
		

		$(this).each(function(){
			$(this).css({
					opacity: c.op,
					filter: "alpha(opacity="+c.op*100+")"
				}).hover(function(){
					$(this).fadeTo(c.durationp,c.oa);
				},function(){
					$(this).fadeTo(c.durationa,c.op);
				})
		});
	},
	
	$.fn.wink = function(durationp,op,oa){

		var c = {
			durationp:durationp? durationp:'slow',
			op:op? op:1.0,
			oa:oa? oa:0.2
		};
		
		$(this).each(function(){
			$(this)	.css({
					opacity: c.op,
					filter: "alpha(opacity="+c.op*100+")"
				}).hover(function(){
				$(this).css({
					opacity: c.oa,
					filter: "alpha(opacity="+c.oa*100+")"
				});
				$(this).fadeTo(c.durationp,c.op);
			},function(){
				$(this).css({
					opacity: c.op,
					filter: "alpha(opacity="+c.op*100+")"
				});
			})
		});
	}
	
})(jQuery);



