				s.linkTrackVars='eVar35';
				s.eVar35='ƒhƒƒCƒ“ŒŸõ';
				s.tl(this,'o','ƒhƒƒCƒ“ŒŸõ');
				return false;
			});
		}
	});

});


function actionSubmitP(fname) {
	
	if ( jQuery("#tld").text() == undefined || jQuery("#tld").text() == "" ) {
		return;
	}
	
	var form = eval('document.' + fname);
	if (form == undefined) {
		return;
	}

	// delete R (hidden)
	var r = form.elements['R'];
	if ( r.length != undefined && r.length > 1 ) {
		for(i = 0; i < r.length; i++) {
			deleteR(r[i].value);
		}
	} else {
		deleteR(r.value);
	}
	
	var tlds = []; 
	jQuery("#list li").each(function() { 
		tlds.push(jQuery(this).text()) 
	}); 
	
	// delete R (p)
	for ( i = 0; i < tlds.length; i++ ) {
		deleteR(tlds.value);
	}
	
	// delete C
	for ( i = 0; i < tlds.length; i++ ) {
		deleteC(tlds.value);
	}
	
	// create R
	if ( r.length != undefined && r.length > 1 ) {
		for(i = 0; i < r.length; i++) {
			createR(form, r[i].value);
		}
	} else {
		createR(form, r.value);
	}
	
	// create R (select)
	createR(form, jQuery("input[name='selectTld']").val());
	
	// create C
	createC(form, jQuery("input[name='selectTld']").val());
	
	form.action = "http://" + form.server.value + "/onamae/domainSeekResult.do";
	form.method = "POST";
	form.submit();
}
function createR( form, tld ) {
	tldsR = document.createElement('input');
	tldsR.setAttribute('type', 'hidden');
	tldsR.id = "tldsR(." + tld + ")";
	tldsR.name = "tldsR(." + tld + ")";
	tldsR.value = tld;
	form.appendChild(tldsR);
}
function createC( form, tld ) {
	tldsC = document.createElement('input');
	tldsC.setAttribute('type', 'hidden');
	tldsC.id = "tldsC(." + tld + ")";
	tldsC.name = "tldsC(." + tld + ")";
	tldsC.value = tld;
	form.appendChild(tldsC);
}
function deleteR( tld ) {
	id = "tldsR(." + tld + ")";
	r = document.getElementById(id);
	if ( r != undefined ) {
		r.parentNode.removeChild(r);
	}
}
function deleteC( tld ) {
	id = "tldsC(." + tld + ")";
	c = document.getElementById(id);
	if ( c != undefined ) {
		c.parentNode.removeChild(c);
	}
}


