				s.eVar35='top_premium-bannertab_pop_n0509';
				s.tl(this,'o','top_premium-bannertab_pop_n0509');
			}else if(tabFlg == 4){//ƒrƒWƒlƒX
				s.eVar35='top_premium-bannertab_holiday_n0509';
				s.tl(this,'o','top_premium-bannertab_holiday_n0509');
			}else if(tabFlg == 5){//HŽ–
				s.eVar35='top_premium-bannertab_baseball_n0509';
				s.tl(this,'o','top_premium-bannertab_baseball_n0509');
			}else if(tabFlg == 1){//’´‚Šz
				s.eVar35='top_premium-bannertab_super_n0509';
				s.tl(this,'o','top_premium-bannertab_super_n0509');
			}
		},500);
		/*-- ƒ^ƒuClick”ƒJƒEƒ“ƒg --*/
		
		return false;
	});

	jQuery('#rotation_inner > dl').hover(function(){
		jQuery("#rotation_inner > dl").attr('style','cursor: pointer;');
		},function(){
