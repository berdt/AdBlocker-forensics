<script type="text/javascript" charset="utf-8">
$(function(){
	$('<img>').attr('src', '//cache.img.gmo.jp/onamae/images/campArea_textarea_highlight.png');
	var srh_highlight_flag = false;
	var srh_focus_flag = false;
	$('#bulk_searchArea_B')
		.on('mouseover focus mouseout blur', function(event){
			if(event.type == 'focus') srh_focus_flag = true;
			if(event.type == 'blur') srh_focus_flag = false;

			switch(event.type) {
				case 'mouseover':
				case 'focus':
					srh_highlight_flag = true;
					break;

				case 'mouseout':
				case 'blur':
					if(! srh_focus_flag) srh_highlight_flag = false;
					break;
			}
			if(srh_highlight_flag) {
				$(this).closest('.main02').addClass('highlight');
			} else {
				$(this).closest('.main02').removeClass('highlight');
			}
		});

	$("label").inFieldLabels();
});
</script>
