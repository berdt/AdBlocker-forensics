<link href="/common/css/count.css" rel="stylesheet" type="text/css" />
<script type="text/javascript">
// <![CDATA[
(function () {
	var CampCountDown = {
		Count: null,
		StartTime: "2017/02/05 18:25:35", 	
		EndTime: "2017/02/06 19:00:00",
		Diff: 0,
		Otime: null,
		Oday: null,
		Ohour: null,
		Ominute: null,
		Osecond: null,
		Omsecond: null,
		RemainSave: 0,
		EndSave: 0,
		Decrese: 0,
		Ddat: "",
		Hdat: "",
		Mdat: "",
		Ts: 24 * 60 * 60 * 1000,
		ShowCount: function() {
			CampCountDown.RemainSave = CampCountDown.EndSave - new Date();
			var Diff = CampCountDown.RemainSave;
			if(Diff <= 0) {
				if(CampCountDown.Decrese !== 0) {
					clearTimeout(CampCountDown.Decrese);
				}
				CampCountDown.End();
			} else {
				var d = CampCountDown.PlusZero(Math.floor(Diff / CampCountDown.Ts)),
			    	h = CampCountDown.PlusZero(Math.floor((Diff % CampCountDown.Ts) / (60 * 60 * 1000))),
			    	m = CampCountDown.PlusZero(Math.floor((Diff % CampCountDown.Ts) / (60 * 1000)) % 60),
			    	s = CampCountDown.PlusZero(Math.floor((Diff % CampCountDown.Ts) / 1000) % 60 % 60),
			    	ms = CampCountDown.PlusZero(Math.floor((Diff % CampCountDown.Ts) / 10) % 100);
				
				if(d != CampCountDown.Ddat) {
			    	CampCountDown.Oday.empty().text(d);
			    	CampCountDown.Ddat = d;
			    }
			    if(h != CampCountDown.Hdat) {
			    	CampCountDown.Ohour.empty().text(h);
			    	CampCountDown.Hdat = h;
			    }
			    if(m != CampCountDown.Mdat) {
			    	CampCountDown.Ominute.empty().text(m);
			    	CampCountDown.Mdat = m;	
			    }
				
				CampCountDown.Osecond.empty().text(s);
				CampCountDown.Omsecond.empty().text(ms);
				
				clearTimeout(CampCountDown.Decrese);
				CampCountDown.Decrese = setTimeout(CampCountDown.ShowCount, 10);
			}

		},
		Start: function() {
			this.Count = jQuery(".dCampCountdown");
			this.Otime =  jQuery(".showCountDown");
			this.Oday = this.Otime.find(".days");
			this.Ohour = this.Otime.find(".hour");
			this.Ominute = this.Otime.find(".minute");
			this.Osecond = this.Otime.find(".second");
			this.Omsecond = this.Otime.find(".msecond");
			this.EndSave = new Date(this.EndTime);
			this.RemainSave = this.EndSave - new Date();
			if(this.RemainSave <= 0) {
				this.End();
			} else {
				this.ShowCount();
				this.Decrese = setTimeout(this.ShowCount, 0);
			}
			
			this.Otime.show();
		},
		End: function() {
			this.Otime.empty().text("キャンペーンは終了しました。");
		},
		PlusZero: function(n) {
			if(n < 10) {
				return "0" + n.toString();
			}
			return n;
		},
		ShowImgNum: function(num) {
			num = num.toString();
			var h = "";
			for(var i = 0; i < num.length; i++) {
				h += '<img src="./images/num'+num.charAt(i)+'.png" alt="'+num.charAt(i)+'" />';
			}
			return h;
			
		}
	};

	if(window.jQuery === undefined) {
		var _script = document.createElement("script");
		_script.setAttribute("type", "text/javascript");
		_script.setAttribute("src", "//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js");
		document.getElementsByTagName("head")[0].appendChild(_script);
		if (typeof window.ActiveXObject === "function") {
			_script.onreadystatechange = function() {
				if(_script.readyState === "loaded" || _script.readyState === "complete") {
					jQuery(document).ready(function() { CampCountDown.Start(); });
				}
			};
		} else {
			_script.onload = function() {
				jQuery(document).ready(function() { CampCountDown.Start(); });
			}
		}
	} else { 
		jQuery(document).ready(function() { CampCountDown.Start(); });
	}
})();
// ]]>	
</script>
					</div>
				</div>
