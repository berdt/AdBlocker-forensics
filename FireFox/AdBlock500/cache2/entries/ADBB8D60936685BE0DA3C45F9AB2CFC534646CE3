<script>
  $(function() {
    var $shopBanner = $('#shopBanner');
    var width = $shopBanner.width();
    var $shiny = $('#shiny');
    var left = $shiny.css('left');
    left = parseInt(left.replace('px', ''), 10);
    var shiny_speed = 800;
    var shiny_interval = 3000;
    (shinybutton = function() {
      $shiny
      	.animate({'left': '+=' + ((width + Math.abs(left)) / 2) + 'px', 'opacity': 1}, shiny_speed / 2, 'easeInQuad')
      	.animate({'left': '+=' + ((width + Math.abs(left)) / 2) + 'px', 'opacity': 0}, shiny_speed / 2, 'easeOutQuad')
      	.animate({'left': left, 'opacity': 0}, 0);
      setTimeout('shinybutton()', shiny_interval);
    })();
  });
</script>
  
