(function(a) {
	a.fn.KinSlideshow = function(B) {
		B = jQuery.extend({
			intervalTime: 5,
			moveSpeedTime: 400,
			moveStyle: "left",
			mouseEvent: "mouseclick",
			isHasTitleBar: true,
			mouseOverStop: true,
			titleBar: {
				titleBar_height: 40,
				titleBar_bgColor: "#000000",
				titleBar_alpha: 0.5
			},
			isHasTitleFont: true,
			titleFont: {
				TitleFont_size: 12,
				TitleFont_color: "#FFFFFF",
				TitleFont_family: "Verdana",
				TitleFont_weight: "bold"
			},
			isHasBtn: true,
			btn: {
				btn_bgColor: "#666666",
				btn_bgHoverColor: "#CC0000",
				btn_fontColor: "#CCCCCC",
				btn_fontHoverColor: "#000000",
				btn_fontFamily: "Verdana",
				btn_borderColor: "#999999",
				btn_borderHoverColor: "#FF0000",
				btn_borderWidth: 1,
				btn_bgAlpha: 0.7
			}
		}, B);
		var f = {
			titleBar_height: 40,
			titleBar_bgColor: "#000000",
			titleBar_alpha: 0.5
		};
		var j = {
			TitleFont_size: 12,
			TitleFont_color: "#FFFFFF",
			TitleFont_family: "Verdana",
			TitleFont_weight: "bold"
		};
		var w = {
			btn_bgColor: "#666666",
			btn_bgHoverColor: "#CC0000",
			btn_fontColor: "#CCCCCC",
			btn_fontHoverColor: "#000000",
			btn_fontFamily: "Verdana",
			btn_borderColor: "#999999",
			btn_borderHoverColor: "#FF0000",
			btn_borderWidth: 1,
			btn_bgAlpha: 0.7
		};
		for (var G in f) {
			if (B.titleBar[G] == undefined) {
				B.titleBar[G] = f[G]
			}
		}
		for (var G in j) {
			if (B.titleFont[G] == undefined) {
				B.titleFont[G] = j[G]
			}
		}
		for (var G in w) {
			if (B.btn[G] == undefined) {
				B.btn[G] = w[G]
			}
		}
		var r = this;
		var b = a(r).selector;
		var A = new Array();
		var p = 0;
		var s = new Array();
		var l = 0;
		var h = B.intervalTime;
		var v;
		var d = true;
		if (isNaN(h) || h <= 1) {
			h = 5
		}
		if (B.moveSpeedTime > 500) {
			B.moveSpeedTime = 500
		}
		if (B.moveSpeedTime < 1) {
			B.moveSpeedTime = 1
		}
		function e() {
			a(r).css({
				visibility: "hidden"
			});
			a(b + " a img").css({
				border: 0
			});
			H();
			D();
			return {
				stop: o,
				start: q
			}
		}
		function H() {
			p = a(b + " a").length;
			s.push(a(b + " a img").width());
			s.push(a(b + " a img").height());
			a(b + " a img").each(function(I) {
				A.push(a(this).attr("alt"))
			});
			a(b + " a").wrapAll("<div id='KSS_content'></div>");
			a("#KSS_content").clone().attr("id", "KSS_contentClone").appendTo(r);
			E();
			F();
			g();
			u();
			z(B.mouseEvent);
			a(r).css({
				visibility: "visible"
			})
		}
		function E() {
			a(r).css({
				width: s[0],
				height: s[1],
				overflow: "hidden",
				position: "relative"
			});
			a(r).append("<div class='KSS_titleBar'></div>");
			var I = B.titleBar.titleBar_height;
			if (isNaN(I)) {
				I = 40
			} else {
				if (I < 25) {
					I = 25
				}
			}
			a(b + " .KSS_titleBar").css({
				height: I,
				width: "100%",
				position: "absolute",
				bottom: 0,
				left: 0
			});
			if (B.isHasTitleBar) {
				a(b + " .KSS_titleBar").css({
					background: B.titleBar.titleBar_bgColor,
					opacity: B.titleBar.titleBar_alpha
				})
			}
		}
		function F() {
			if (B.isHasTitleFont) {
				a(b + " .KSS_titleBar").append("<h2 class='title' style='margin:3px 0 0 6px;padding:0;'></h2>");
				a(b + " .KSS_titleBar .title").css({
					fontSize: B.titleFont.TitleFont_size,
					color: B.titleFont.TitleFont_color,
					fontFamily: B.titleFont.TitleFont_family,
					fontWeight: B.titleFont.TitleFont_weight
				});
				x(0)
			}
		}
		function g() {
			if (B.btn.btn_borderWidth > 2) {
				B.btn.btn_borderWidth = 2
			}
			if (B.btn.btn_borderWidth < 0 || isNaN(B.btn.btn_borderWidth)) {
				B.btn.btn_borderWidth = 0
			}
			if (B.isHasBtn && p >= 2) {
				a(r).append("<div class='KSS_btnBox' style='position:absolute;right:10px;bottom:5px; z-index:100'></div>");
				var I = "";
				for (i = 1; i <= p; i++) {
					I += "<li></li>"
				}
				I = "<ul id='btnlistID' style='margin:0;padding:0; overflow:hidden'>" + I + "</ul>";
				a(b + " .KSS_btnBox").append(I);
				a(b + " .KSS_btnBox #btnlistID li").css({
					listStyle: "none",
					"float": "left",
					width: 8,
					height: 8,
					borderWidth: B.btn.btn_borderWidth,
					borderColor: B.btn.btn_borderColor,
					borderStyle: "solid",
					background: B.btn.btn_bgColor,
					textAlign: "center",
					cursor: "pointer",
					marginLeft: 3,
					fontSize: 12,
					fontFamily: B.btn.btn_fontFamily,
					lineHeight: "8px",
					opacity: B.btn.btn_bgAlpha,
					color: B.btn.btn_fontColor,
					"border-radius":4
				});
				a("#btnlistID li:eq(0)").css({
					background: B.btn.btn_bgHoverColor,
					borderColor: B.btn.btn_borderHoverColor,
					color: B.btn.btn_fontHoverColor
				})
			}
		}
		function u() {
			switch (B.moveStyle) {
			case "left":
				k();
				break;
			case "right":
				n();
				break;
			case "up":
				y();
				break;
			case "down":
				t();
				break;
			default:
				B.moveStyle = "left";
				k()
			}
		}
		function k() {
			a(b + " div:lt(2)").wrapAll("<div id='KSS_moveBox'></div>");
			a("#KSS_moveBox").css({
				width: s[0],
				height: s[1],
				overflow: "hidden",
				position: "relative"
			});
			a("#KSS_content").css({
				"float": "left"
			});
			a("#KSS_contentClone").css({
				"float": "left"
			});
			a(b + " #KSS_moveBox div").wrapAll("<div id='KSS_XposBox'></div>");
			a(b + " #KSS_XposBox").css({
				"float": "left",
				width: "2000%"
			});
			v = setInterval(function() {
				m(B.moveStyle)
			}, h * 1000 + B.moveSpeedTime)
		}
		function n() {
			a(b + " div:lt(2)").wrapAll("<div id='KSS_moveBox'></div>");
			a("#KSS_moveBox").css({
				width: s[0],
				height: s[1],
				overflow: "hidden",
				position: "relative"
			});
			a("#KSS_content").css({
				"float": "left"
			});
			a("#KSS_contentClone").css({
				"float": "left"
			});
			a(b + " #KSS_moveBox div").wrapAll("<div id='KSS_XposBox'></div>");
			a(b + " #KSS_XposBox").css({
				"float": "left",
				width: "2000%"
			});
			a("#KSS_contentClone").html("");
			a("#KSS_content a").wrap("<span></span>");
			a("#KSS_content a").each(function(J) {
				a("#KSS_contentClone").prepend(a("#KSS_content span:eq(" + J + ")").html())
			});
			a("#KSS_content").html(a("#KSS_contentClone").html());
			var I = (p - 1) * s[0];
			a("#KSS_moveBox").scrollLeft(I);
			v = setInterval(function() {
				m(B.moveStyle)
			}, h * 1000 + B.moveSpeedTime)
		}
		function y() {
			a(b + " div:lt(2)").wrapAll("<div id='KSS_moveBox'></div>");
			a("#KSS_moveBox").css({
				width: s[0],
				height: s[1],
				overflow: "hidden",
				position: "relative"
			});
			a("#KSS_moveBox").animate({
				scrollTop: 0
			}, 1);
			v = setInterval(function() {
				m(B.moveStyle)
			}, h * 1000 + B.moveSpeedTime)
		}
		function t() {
			a(b + " div:lt(2)").wrapAll("<div id='KSS_moveBox'></div>");
			a("#KSS_moveBox").css({
				width: s[0],
				height: s[1],
				overflow: "hidden",
				position: "relative"
			});
			a("#KSS_contentClone").html("");
			a("#KSS_content a").wrap("<span></span>");
			a("#KSS_content a").each(function(J) {
				a("#KSS_contentClone").prepend(a("#KSS_content span:eq(" + J + ")").html())
			});
			a("#KSS_content").html(a("#KSS_contentClone").html());
			var I = (p - 1) * s[1];
			a("#KSS_moveBox").animate({
				scrollTop: I
			}, 1);
			v = setInterval(function() {
				m(B.moveStyle)
			}, h * 1000 + B.moveSpeedTime)
		}
		function m(J) {
			switch (J) {
			case "left":
				if (l >= p) {
					l = 0;
					a("#KSS_moveBox").scrollLeft(0);
					a("#KSS_moveBox").animate({
						scrollLeft: s[0]
					}, B.moveSpeedTime)
				} else {
					sp = (l + 1) * s[0];
					if (a("#KSS_moveBox").is(":animated")) {
						a("#KSS_moveBox").stop();
						a("#KSS_moveBox").animate({
							scrollLeft: sp
						}, B.moveSpeedTime)
					} else {
						a("#KSS_moveBox").animate({
							scrollLeft: sp
						}, B.moveSpeedTime)
					}
				}
				x(l + 1);
				break;
			case "right":
				var I = (p - 1) * s[0];
				if (l >= p) {
					l = 0;
					a("#KSS_moveBox").scrollLeft(I + s[0]);
					a("#KSS_moveBox").animate({
						scrollLeft: I
					}, B.moveSpeedTime)
				} else {
					if (d) {
						l++;
						d = false
					}
					sp = I - (l * s[0]);
					if (a("#KSS_moveBox").is(":animated")) {
						a("#KSS_moveBox").stop();
						a("#KSS_moveBox").animate({
							scrollLeft: sp
						}, B.moveSpeedTime)
					} else {
						a("#KSS_moveBox").animate({
							scrollLeft: sp
						}, B.moveSpeedTime)
					}
				}
				x(l);
				break;
			case "up":
				if (l >= p) {
					l = 0;
					a("#KSS_moveBox").scrollTop(0);
					a("#KSS_moveBox").animate({
						scrollTop: s[1]
					}, B.moveSpeedTime)
				} else {
					sp = (l + 1) * s[1];
					if (a("#KSS_moveBox").is(":animated")) {
						a("#KSS_moveBox").stop();
						a("#KSS_moveBox").animate({
							scrollTop: sp
						}, B.moveSpeedTime)
					} else {
						a("#KSS_moveBox").animate({
							scrollTop: sp
						}, B.moveSpeedTime)
					}
				}
				x(l + 1);
				break;
			case "down":
				var I = (p - 1) * s[1];
				if (l >= p) {
					l = 0;
					a("#KSS_moveBox").scrollTop(I + s[1]);
					a("#KSS_moveBox").animate({
						scrollTop: I
					}, B.moveSpeedTime)
				} else {
					if (d) {
						l++;
						d = false
					}
					sp = I - (l * s[1]);
					if (a("#KSS_moveBox").is(":animated")) {
						a("#KSS_moveBox").stop();
						a("#KSS_moveBox").animate({
							scrollTop: sp
						}, B.moveSpeedTime)
					} else {
						a("#KSS_moveBox").animate({
							scrollTop: sp
						}, B.moveSpeedTime)
					}
				}
				x(l);
				break
			}
			l++
		}
		function x(I) {
			if (I == p) {
				I = 0
			}
			if (B.isHasTitleFont) {
				a(b + " .KSS_titleBar h2").html(A[I])
			}
			a("#btnlistID li").each(function(J) {
				if (J == I) {
					a(this).css({
						background: B.btn.btn_bgHoverColor,
						borderColor: B.btn.btn_borderHoverColor,
						color: B.btn.btn_fontHoverColor
					})
				} else {
					a(this).css({
						background: B.btn.btn_bgColor,
						borderColor: B.btn.btn_borderColor,
						color: B.btn.btn_fontColor
					})
				}
			})
		}
		function z(I) {
			switch (I) {
			case "mouseover":
				C();
				break;
			case "mouseclick":
				c();
				break;
			default:
				c()
			}
		}
		function C() {
			a("#btnlistID li").mouseover(function() {
				var I = a("#btnlistID li").index(a(this));
				switch (B.moveStyle) {
				case "left":
					l = I - 1;
					break;
				case "right":
					if (d) {
						l = I - 1;
						break
					} else {
						l = I;
						break
					}
				case "up":
					l = I - 1;
					break;
				case "down":
					if (d) {
						l = I - 1;
						break
					} else {
						l = I;
						break
					}
				}
				m(B.moveStyle);
				a("#btnlistID li").each(function(J) {
					if (J == I) {
						a(this).css({
							background: B.btn.btn_bgHoverColor,
							borderColor: B.btn.btn_borderHoverColor,
							color: B.btn.btn_fontHoverColor
						})
					} else {
						a(this).css({
							background: B.btn.btn_bgColor,
							borderColor: B.btn.btn_borderColor,
							color: B.btn.btn_fontColor
						})
					}
				})
			})
		}
		function c() {
			a("#btnlistID li").click(function() {
				var I = a("#btnlistID li").index(a(this));
				switch (B.moveStyle) {
				case "left":
					l = I - 1;
					break;
				case "right":
					if (d) {
						l = I - 1;
						break
					} else {
						l = I;
						break
					}
				case "up":
					l = I - 1;
					break;
				case "down":
					if (d) {
						l = I - 1;
						break
					} else {
						l = I;
						break
					}
				}
				m(B.moveStyle);
				a("#btnlistID li").each(function(J) {
					if (J == I) {
						a(this).css({
							background: B.btn.btn_bgHoverColor,
							borderColor: B.btn.btn_borderHoverColor,
							color: B.btn.btn_fontHoverColor
						})
					} else {
						a(this).css({
							background: B.btn.btn_bgColor,
							borderColor: B.btn.btn_borderColor,
							color: B.btn.btn_fontColor
						})
					}
				})
			})
		}
		function o() {
			clearInterval(v)
		}
		function q() {
			o();
			v = setInterval(function() {
				m(B.moveStyle)
			}, h * 1000 + B.moveSpeedTime)
		}
		function D() {
			if (B.mouseOverStop) {
				a(r).mouseover(o);
				a(r).mouseout(q)
			} else {
				a("#btnlistID li").mouseover(o);
				a("#btnlistID li").mouseout(q)
			}
		}
		return e()
	}
