    script.src = '//d3c3cq33003psk.cloudfront.net/opentag-117668-1601050.js';
} else if( (window.location.hostname.indexOf('cafepress.com') > -1) || (window.location.hostname.indexOf('.dev') > -1) ){
    script.src = '//d3c3cq33003psk.cloudfront.net/opentag-117668-1601028.js';
}

head.appendChild(script);

