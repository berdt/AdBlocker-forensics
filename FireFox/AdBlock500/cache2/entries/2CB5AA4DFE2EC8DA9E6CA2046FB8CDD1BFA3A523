			<script>
				var aspectRadio = function() {
					var img = document.getElementById("banner-desktop");
					if (window.innerWidth <= 520) {
						//Mobile
						img = document.getElementById("banner-mobile");
					} else if (window.innerWidth >= 521 && window.innerWidth <= 960) {
						//Tablet
						img = document.getElementById("banner-tablet");
					}
					//Image Resize by keeping aspect Radio
					var aspecRadio = img.width / img.height;
					var arWidth = document.getElementsByClassName("uk-slideshow")[0].offsetWidth;
					var arHeight = arWidth / aspecRadio;
					img.width = arWidth;
					img.height = arHeight;
					document.getElementById("hero-block").style.height = arHeight;
					document.getElementsByClassName("uk-slideshow")[0].style.height = arHeight;
				}
				aspectRadio();
			</script>
