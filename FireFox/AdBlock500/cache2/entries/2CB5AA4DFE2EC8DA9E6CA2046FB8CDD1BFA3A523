<script type="text/javascript" src="http://content.cpcache.com/minify/js/global.js?v=8981341b3667f90e7258550562e31be7"></script>
<script type="text/javascript">
	//This loads the global deferred file. For now this includes tracking/facebook/3rd party scripts
	//The media query checks if screen width is below 570PX. if it is, then it doesnt load the deferred file, otherwise it does.
	//-saif
	$(document).ready(function(){
		cafepress.globalHeader.matchMedia(570, function(mq){
			if(!mq.matches){
				if (!window.deferLoaded){
					window.deferLoaded = true;
					if (!window.mobileDeferLoaded){
						$.getScript('http://content.cpcache.com/minify/js/globalDeferred.js');
					} else {
						$.getScript('http://content.cpcache.com/minify/js/globalDeferredExtended.js');
					}
				}
			} else {
				if (!window.deferLoaded && !window.mobileDeferLoaded){
					window.mobileDeferLoaded = true;
					$.getScript('http://content.cpcache.com/minify/js/globalDeferredMobile.js');
				}
			}
		});
	});
</script>
 
	
    
    
	

