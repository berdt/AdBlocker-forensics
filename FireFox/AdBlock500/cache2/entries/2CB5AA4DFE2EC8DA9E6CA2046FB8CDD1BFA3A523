<script>
	function loadInlineJSCode() {
		$(document).ready(function() {
			$('#collapsed-button').hide();
			$('#button-click').click(function() {
				$('#button-toggle').fadeToggle(800).toggleClass('show');
				$('#button-click').toggleClass('orange');
				if ($('#button-toggle').hasClass('show')) {
					$('#button-click').hide().appendTo($('#categories')).fadeIn(800).html('Fewer Categories &#9650;');
					$('#collapsed-button').fadeToggle(1000);
				} else {
					$('#collapsed-button').hide();
					$('#button-click').hide().fadeIn(1000).html('More Gift Categories &#9660;');
				};
			});
		});
	};
	(function() {
		var checkInlinejQueryInterval = setInterval(checkInlinejQueryPresent, 100);
		function checkInlinejQueryPresent() {
			if (window.jQuery) {
				clearInterval(checkInlinejQueryInterval);
				loadInlineJSCode();
			}
		}
	})();
</script>

