<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js" type="text/javascript"></script>
<script>window.jQuery || document.write("<script src='/consent/script/jquery-1.8.2.min.js'>\x3C/script>")</script>

<script>

var levels = [];
levels[0] = ['essential', 'functional', 'advertising', 'analytics'];
levels[1] = ['advertising_network'];
levels[2] = ['social', 'advertising_targeting'];

function getCookie(c_name) {
    var i, x, y, ARRcookies = document.cookie.split(";");
    for(i = 0; i < ARRcookies.length; i++) {
        x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
        y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
        x = x.replace(/^\s+|\s+$/g, "");
        if(x == c_name) {
            return unescape(y);
        }
    }
}

function detectLevel() {
  var cookies = (getCookie('cc') || '').split('.');
  for (var i = levels.length - 1; i >= 0; i--) {
    var n = 0;
    $.each(levels[i], function(_, cat) {
      if($.inArray(cat, cookies) >= 0) {
        ++n;
      } else {
        return false;
      }
    })
    if(n==levels[i].length) {
      return 1+i;
    }
  };
  return null;
}

function changeLevel(level) {
  var preset = 3;
  var currentLevel = detectLevel() || preset;
  level = level || currentLevel;
  levelID = 'cookie-level-'+(level);
  //$('#'+levelID).attr('checked', true);
  $('.option').removeClass('selected');
  $('.option img').attr('src', '/consent/img/not_pressed.png');
  $('#'+levelID).addClass('selected');
  $('#'+levelID + ' img').attr('src', '/consent/img/pressed.png');
  $('#level').attr('value', levelID);
  $('#will').find('li').remove();
  $('#will').append($('#' + levelID + ' .will').text())
  $('#will-not').find('li').remove();
  $('#will-not').append($('#' + levelID + ' .will-not').text())
  detectLevel() && level < currentLevel ? $('#delete-warning').show() : $('#delete-warning').hide()
  $('#tip').text(level == preset ? 'de normale gebruikerservaring' : 'uw huidige selectie');
  if (level == 3) {
	  $('#will-not').hide();
  } else {
	  $('#will-not').show();
  }

  // reset and select levels
  $('input[type=checkbox]').attr('checked', false);
  while (level > 0) {
  	$(levels[--level]).each(function(i, elm){
  	  $('#' + elm).attr('checked', true);
	});
  }
}

function cookieCheck() {
  var cookieEnabled = (navigator.cookieEnabled) ? true : false;
  if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled) {
    document.cookie="__cverify=1";
    cookieEnabled = (document.cookie.indexOf("__cverify") != -1) ? true : false;
    var expires = new Date(1984,8,24);
    document.cookie = "__cverify=1;expires=" + expires.toUTCString();
  }
  return cookieEnabled;
}

$(function(){
    /*
     * Respond to <enter> keystroke
     */
    $(document).keydown(function(e) {
        if (e.keyCode == 13) { //enter
            e.preventDefault();
            $('form').submit();
        }
        if(e.keyCode == 27) { //escape
          e.preventDefault();
          $('.cancel').click();
        }
    });

    /*
     * Activate "advanced" link
     */
    $('div.advanced').on('click', function(ev) {
      window.location.hash = 'geavanceerd';
      ev.preventDefault();
      $('.intro').hide();
      $('.geavanceerd').show();
      $('div.advanced').hide();
      $('.cancel').show();
	  $('.header .close').show();
    });
    $('.cancel').on('click', function(ev) {
      ev.preventDefault();
      $('.geavanceerd').hide();
      $('.intro').show();
      $(this).hide();
      $('div.advanced').show();
    });
    $('.header .close').on('click', function() {
      $('.cancel').click();
    })
    /*
     * Change list contents on radio select
     */
    $('.cookie-level-select').on('click', function(ev) {
      changeLevel(parseInt($(ev.currentTarget).attr('id').slice(-1)));
    })
    /*
     * Set default level
     */
    changeLevel();
    /*
     * Respect hash for advanced page
     */
    if(window.location.hash == '#geavanceerd') {
      $('.cancel').off();
      $('.cancel').on('click', function(ev) {
        ev.preventDefault();
        window.location.href = 'http://www.telegraaf.nl/?';
      })
      $('.header .close').show();
      $('div.advanced').click();
    }
    /*
     * Maybe show flashes
     */
    
      
    
    /*
     * Check if cookies are even accepted
     */
    if(!cookieCheck()) {
      $('.flashes .messages').append($('<li></li>').text('Uw browser accepteert geen cookies. Hierdoor is het niet mogelijk uw voorkeur op te slaan. Pas de instellingen van uw browser aan en probeer het daarna opnieuw.'));
      $('.flashes').show();
    }
});
</script>
